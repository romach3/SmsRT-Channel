{
    "name": "laravel-notification-channels/sms-rt",
    "description": "SMS-channel for sms.rt.ru (Rostelecom)",
    "homepage": "https://github.com/laravel-notification-channels/SmsRT",
    "license": "MIT",
    "authors": [
        {
            "name": "romach3",
            "email": "romach3@gmail.com",
            "homepage": "https://github.com/romach3",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/notifications": "~5.5 || ~6.0 || ~7.0",
        "illuminate/support": "~5.5 || ~6.0 || ~7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0",
        "vlucas/phpdotenv": "^5.1",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\SmsRT\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\SmsRT\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    }
}
